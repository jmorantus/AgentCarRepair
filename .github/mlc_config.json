{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/yourusername/"
    },
    {
      "pattern": "^https://your-endpoint"
    },
    {
      "pattern": "^http://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://docs.microsoft.com"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
      }
    }
  ],
  "timeout": 30000,
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttpStatus": [
    400,
    401,
    403,
    404,
    500,
    502,
    503,
    504
  ]
}
